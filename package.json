{
  "name": "neuburg-backend",
  "version": "0.0.1",
  "main": "dist",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "flow-typed": "flow-typed install",
    "build": "babel src --out-dir dist",
    "build:dev": "babel src --source-maps --out-dir dist",
    "start": "cross-env NODE_ENV=production node dist",
    "start:dev": "cross-env NODE_ENV=development nodemon --watch src --exec \"babel-node src\"",
    "flow": "flow",
    "lint": "eslint src; flow",
    "lint:fix": "eslint src --fix "
  },
  "license": "MIT",
  "dependencies": {
    "@types/nodemailer": "^4.6.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "commander": "^2.15.1",
    "cosmiconfig": "^5.0.4",
    "express": "^4.16.3",
    "express-validator": "^5.2.0",
    "http-status-codes": "^1.3.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "mongoose": "^5.1.1",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.4",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "cross-env": "^5.1.5",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "flow-bin": "^0.72.0",
    "flow-typed": "^2.4.0",
    "nodemon": "^1.17.4"
  }
}
